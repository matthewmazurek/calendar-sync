{
    "name": "mazurek",
    "version": "1.0",
    "extends": "default",
    "locations": {
        "work": {
            "address": "1403 29 St NW, Calgary AB T2N 2T9, Canada",
            "geo": [
                51.065389,
                -114.133306
            ],
            "apple_title": "Foothills Medical Centre"
        }
    },
    "defaults": {
        "busy": false,
        "location": "work",
        "time_periods": {
            "AM": [
                "0800",
                "1200"
            ],
            "PM": [
                "1230",
                "1630"
            ]
        }
    },
    "types": {
        "on_call": {
            "match": "on call",
            "label": "^(.+?)\\s+on call",
            "consolidate": {
                "group_by": "label",
                "pattern_aware": true
            },
            "overnight": {
                "as": "all_day",
                "format": "{label} on call {time_range}"
            }
        },
        "endoscopy": {
            "match": [
                "endoscopy",
                "endo",
                "DTP"
            ],
            "consolidate": false,
            "time_periods": {
                "AM": [
                    "0730",
                    "1200"
                ],
                "PM": [
                    "1230",
                    "1630"
                ]
            }
        },
        "ccsc": {
            "match": "ccsc",
            "consolidate": false,
            "time_periods": {
                "AM": [
                    "0730",
                    "1200"
                ],
                "PM": [
                    "1230",
                    "1630"
                ]
            }
        },
        "clinic": {
            "match": "clinic",
            "consolidate": false
        },
        "admin": {
            "match": "admin",
            "location": "office",
            "consolidate": false
        },
        "holiday": {
            "suppress": true
        },
        "vacation": {
            "match": [
                "vacation",
                "away"
            ],
            "location": null
        }
    }
}